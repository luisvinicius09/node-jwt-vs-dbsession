		"@types/supertest": "6.0.2",
		"cross-env": "7.0.3",
		"drizzle-kit": "0.20.14",
		"eslint": "8.45.0",
		"npm-run-all": "4.1.5",
		"supertest": "6.3.4",
		"tsup": "^7.1.0",
		"tsx": "^3.12.7",
