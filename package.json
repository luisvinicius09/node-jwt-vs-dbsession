{
	"name": "fastify-ts-bare-starter",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=production node dist/bootstrap.js",
		"start:dev": "cross-env NODE_ENV=dev tsx watch src/bootstrap.ts",
		"build": "tsup src --out-dir dist",
		"db:migrate": "drizzle-kit generate:pg",
		"db:push": "drizzle-kit push:pg",
		"db:studio": "drizzle-kit studio",
		"test:create-drizzle-environment": "npm link ./drizzle/vitest-environment-drizzle",
		"test:install-drizzle-environment": "npm link vitest-environment-drizzle",
		"pretest:e2e": "run-s test:create-drizzle-environment test:install-drizzle-environment",
		"test:e2e": "cross-env NODE_ENV=test vitest run"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
