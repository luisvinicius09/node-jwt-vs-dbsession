		"@fastify/cookie": "9.3.1",
		"@fastify/jwt": "8.0.0",
		"@fastify/view": "9.0.0",
		"bcrypt": "5.1.1",
		"dotenv": "16.3.1",
		"drizzle-orm": "0.30.7",
		"fastify": "^4.20.0",
		"jsonwebtoken": "9.0.2",
		"pg": "8.11.5",
		"postgres": "3.4.4",
		"pug": "3.0.2",
		"zod": "3.21.4"
	},
	"devDependencies": {
		"@rocketseat/eslint-config": "2.1.0",
